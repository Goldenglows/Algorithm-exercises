public class SnapshotArray {
    private int length;
    private List <int[]> snapshots;
    private int[] currentSnapshot;
    private int snapCount = 0; 

    public SnapshotArray(int length) {
        this.length = length;
        snapshots = new List<int[]>();
        currentSnapshot = new int[length];
    }
    
    public void Set(int index, int val) {
        currentSnapshot[index] = val;
    }


    public int Snap() {
        snapshots.Add((int[])currentSnapshot.Clone());
        return snapCount++;
    }

    public int Get(int index, int snap_id) {
        if(snap_id >= snapshots.Count)
            return 0;
        return snapshots[snap_id][index];
    }
}

